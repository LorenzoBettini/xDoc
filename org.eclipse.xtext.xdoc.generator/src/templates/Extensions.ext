escapeLatexChars(String s):
	s.replaceAll("\\#", "\\\\#").replaceAll("\\{", "\\\\{")
		.replaceAll("\\}", "\\\\}").replaceAll("\\\\", "\\\\textbackslash{}")
		.replaceAll("_", "\\\\_").replaceAll("\\^", "\\\\textasciicircum{}")
		.replaceAll("&", "\\\\&");

unescapeXdocChars(String s):
	s.replaceAll("\\\\\\[", "[").replaceAll("\\\\\\]", "]");
